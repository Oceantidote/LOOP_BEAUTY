if(<%= @address.delivery_address %> == true) {
  deliveryHolder.value = <%= @address.user.delivery_addresses.last.id %>
  deliveryAddressShow.innerHTML = <%= @address.user.delivery_addresses.last.street %>
  deliveryAddressShow.classList.remove('d-none')
  newDeliveryInput.classList.add('d-none')
  deliveryButtons.classList.remove('d-none')
} else {
  billingHolder.value = <%= @address.user.billing_addresses.last.id %>
  billingAddressShow.innerHTML = <%= @address.user.billing_addresses.last.street %>
  billingAddressShow.classList.remove('d-none')
  newBillingInput.classList.add('d-none')
  billingButtons.classList.remove('d-none')
}
