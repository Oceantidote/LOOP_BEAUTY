<%= simple_form_for @influencer, url: admin_influencers_path do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :instagram %>
  <%= f.input :youtube %>
  <%= f.input :avatar_photo %>
  <%= f.input :cover_photo %>
  <%= f.input :qa_photo %>
  <%= f.input :description %>
  <%= f.input :password %>
  <%= f.submit 'Create Influencer Account' %>
<% end %>
