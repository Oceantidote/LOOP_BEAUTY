<div class="dashboard-wrapper">
  <div class="dashboard-container">
    <div class="dash-menu-container">
      <%= render 'shared/dash' %>
    </div>
    <div class="dash-content-container">
      <h2 class="insider-info-title mlb-title text-center">Hello <%= current_user.first_name %> <%= current_user.last_name %></h2>
      <hr>
      <div class="wishlist-product-cards-container">
        <%= render 'shared/wishlist_product_card', wishlist_product: @wishlist_product %>
      </div>
    </div>
  </div>
</div>
