<div class="dashboard-wrapper">
  <div class="dashboard-container">
    <div class="dash-menu-container">
      <%= render 'shared/influencer_dash' %>
    </div>
    <div class="dash-content-container">
      <h2 class="insider-info-title mlb-title text-center">Share</h2>
      <hr>
      <div class="dash-home-options">
        <%= link_to new_lookbook_path, class: "dash-home-option-links" do %>
          <div class="each-dash-home-option">
            <%= image_tag 'basket.png' %>
            <h5>Upload Lookbook</h5>
          </div>
        <% end %>
        <div class="dash-home-options-line-break"></div>
        <%= link_to new_tutorial_path, class: "dash-home-option-links" do %>
          <div class="each-dash-home-option">
            <%= image_tag 'dash-wish.png' %>
            <h5>Upload Tutorial</h5>
          </div>
        <% end %>
        <div class="dash-home-options-line-break"></div>
        <%= link_to user_uploads_path, class: "dash-home-option-links" do %>
          <div class="each-dash-home-option">
            <%= image_tag 'default-user.png' %>
            <h5>Check Content Status</h5>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
